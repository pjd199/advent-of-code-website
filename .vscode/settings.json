{
  "workbench.colorTheme": "Visual Studio Dark",
  "files.associations": {
    "*.mdx": "mdx"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "eslint.options": {
    "extensions": [".js", ".jsx", ".md", ".mdx", ".ts", ".tsx"]
  },
  "eslint.validate": [
    "markdown",
    "mdx",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
